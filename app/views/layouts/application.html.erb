<body class="bg-gray-400 font-sans leading-normal tracking-normal">



  <nav class="flex items-center justify-between flex-wrap bg-white-800 p-6  w-full z-10 top-0 ">
    <div class="flex items-center flex-shrink-0 text-white mr-6">
      <a class="text-white no-underline hover:text-white hover:no-underline" href="#">
       <%= link_to root_path do %>
          <%= image_tag "logo.svg", :size => "180x110" %>
       <% end %>
      </a>
    </div>
    <div class="block lg:hidden">
      <button id="nav-toggle" class="flex items-center px-3 py-2 border rounded text-gray-500 border-white-600 hover:text-white hover:border-white">
        <svg class="fill-current h-3 w-3" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><title>Menu</title><path d="M0 3h20v2H0V3zm0 6h20v2H0V9zm0 6h20v2H0v-2z"/></svg>
      </button>

    </div>
    <div >

      <ul class="list-reset lg:flex justify-end flex-1 items-center">
<% if user_signed_in? %>
          <%= link_to "Sign out", destroy_user_session_path, method: :delete, :class=> 'inline-block text-gray-600 no-underline hover:text-gray-200 hover:text-underline py-2 px-4' %>

          <%= link_to "Profil", user_path(current_user.id), :class=> 'inline-block text-gray-600 no-underline hover:text-gray-200 hover:text-underline py-2 px-4'%>

           <%= link_to "Liste des clubs", clubs_path , :class=> 'inline-block text-gray-600 no-underline hover:text-gray-200 hover:text-underline py-2 px-4'%>

           <%= link_to "Liste des recrutements", recrutements_path , :class=> 'inline-block text-gray-600 no-underline hover:text-gray-200 hover:text-underline py-2 px-4'%>
  <% else %>    
            <%= link_to "Se connecter", new_user_session_path,:class=> 'inline-block text-gray-600 no-underline hover:text-gray-200 hover:text-underline py-2 px-4' %>     
            <%= link_to "S'inscrire", new_user_registration_path, :class=> 'inline-block text-gray-600 no-underline hover:text-gray-200 hover:text-underline py-2 px-4' %>   
      <% end %>
      </ul>
    </div>
  </nav>

  <!--Container-->
  <div class="container shadow-lg mx-auto bg-white mt-24 md:mt-18">

  </div>

  <script>
    //Javascript to toggle the menu
    document.getElementById('nav-toggle').onclick = function(){
      document.getElementById("nav-content").classList.toggle("hidden");
    }
  </script>

  <%= yield %>


