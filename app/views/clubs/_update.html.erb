
   <%= form_tag url_for(action: 'update'), method: "patch" do |form|%>
    <input type="hidden" name="authenticity_token" value=<%= form_authenticity_token %> >


  <div class="field">
    <%= label_tag :name %>
    <%= text_area_tag :name, @club.name %>
  </div>


   <div class="field">
    <%= label_tag :category %>
    <%= text_area_tag :category, @club.category %>
  </div>
  <div class="form-group">
   <label>Quarter</label>
   <%=select_tag 'quarter_id', options_for_select(@quarters.collect{ |t| [t.name, t.id] }), class:'form-control'%>
 </div>

  
<div class="field">
    <input type="hidden" name="club_id" value="<%= @club.id%>">
  </div>



  <div class="actions">
    <%= submit_tag %>
  </div>
<% end %>
